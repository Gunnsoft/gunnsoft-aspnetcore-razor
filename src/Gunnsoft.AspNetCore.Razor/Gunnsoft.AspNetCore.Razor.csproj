<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <Title>Gunnsoft.AspNetCore.Razor</Title>
    <AssemblyName>Gunnsoft.AspNetCore.Razor</AssemblyName>
    <PackageId>Gunnsoft.AspNetCore.Razor</PackageId>
    <Authors>Andrew Gunn</Authors>
    <Company>Gunnsoft</Company>
    <AssemblyTitle>Gunnsoft.AspNetCore.Razor</AssemblyTitle>
    <Description>
      Gunnsoft ASP.NET Core Razor
    </Description>
    <Copyright>Copyright Â© Gunnsoft 2018</Copyright>
    <PackageTags>Gunnsoft;AspNetCore;Razor</PackageTags>
    <PackageProjectUrl>https://github.com/Gunnsoft/gunnsoft-aspnetcore.razor</PackageProjectUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/Gunnsoft/gunnsoft-aspnetcore-razor</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Razor" Version="2.0.3" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.TagHelpers" Version="2.0.3" />
  </ItemGroup>

</Project>
